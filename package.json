{
    "name": "haml",
    "displayName": "Better Haml",
    "description": "Better Haml syntax highlighting, auto close and suggestions",
    "version": "0.0.5",
    "publisher": "karunamurti",
    "engines": {
        "vscode": "^1.0.0"
    },
    "icon": "images/haml.png",
    "categories": [
        "Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [{
            "id": "haml",
            "aliases": ["Ruby Haml", "haml"],
            "extensions": [".haml"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "haml",
            "scopeName": "text.haml",
            "path": "./syntaxes/haml.tmLanguage"
        }
        ],
        "snippets": [{
            "language": "haml",
            "path": "./snippets/snippets.json"
        }]
    }
}
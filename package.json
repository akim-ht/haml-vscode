{
    "name": "haml",
    "displayName": "Better Haml",
    "description": "Better Haml syntax highlighting, auto close and suggestions",
    "version": "1.0.0",
    "publisher": "karunamurti",
    "engines": {
        "vscode": "^1.5.0"
    },
    "icon": "images/haml.png",
    "categories": [
        "Languages",
        "Snippets"
    ],
    "license": "MIT",
    "keywords": [
        "haml",
        "Ruby Haml",
        "Haml",
        "HAML"
    ],
    "main": "./haml",
    "activationEvents": [
        "onLanguage:haml"
    ],
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "contributes": {
        "languages": [
            {
                "id": "haml",
                "aliases": [
                    "Ruby Haml",
                    "Haml",
                    "haml"
                ],
                "extensions": [
                    ".haml"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "haml",
                "scopeName": "text.haml",
                "path": "./syntaxes/haml.json"
            }
        ],
        "snippets": [
            {
                "language": "haml",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/karuna/vscode-haml.git"
    },
    "galleryBanner": {
        "color": "#888888",
        "theme": "dark"
    },
    "homepage": "https://github.com/karuna/vscode-haml",
    "bugs": {
        "url": "https://github.com/karuna/vscode-haml/issues",
        "email": "karuna.murti@gmail.com"
    },
    "extensionDependencies": [
        "rebornix.Ruby"
    ],
    "__metadata": {
        "id": "31dab038-d2f7-47c2-894e-5c35684dbd0e",
        "publisherId": "14fb4112-a640-4645-8750-f1fab9c5e87a",
        "publisherDisplayName": "Karuna Murti"
    },
    "devDependencies": {
        "vscode": "^1.0.0",
        "eslint": "^3.6.0"
    }
}
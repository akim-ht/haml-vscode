{
    "name": "haml",
    "displayName": "Better Haml",
    "description": "Better Haml syntax highlighting, auto close and suggestions",
    "version": "0.3.0",
    "publisher": "karunamurti",
    "engines": {
        "vscode": "^1.0.0"
    },
    "icon": "images/haml.png",
    "categories": [
        "Languages",
        "Snippets"
    ],
    "license": "SEE LICENSE IN LICENSE.md",
    "activationEvents": [
        "onLanguage:haml"
    ],
    "contributes": {
        "languages": [
            {
                "id": "haml",
                "aliases": [
                    "Ruby Haml",
                    "haml"
                ],
                "extensions": [
                    ".haml"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "haml",
                "scopeName": "text.haml",
                "path": "./syntaxes/haml.tmLanguage"
            }
        ],
        "snippets": [
            {
                "language": "haml",
                "path": "./snippets/snippets.json"
            }
        ]
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/karuna/ruby_haml_for_vscode.git"
    },
    "galleryBanner": {
        "color": "#0000FF",
        "theme": "dark"
    },
    "homepage": "https://github.com/karuna/ruby_haml_for_vscode",
    "bugs": {
        "url": "https://github.com/karuna/ruby_haml_for_vscode/issues",
        "email": "karuna.murti@gmail.com"
    }
}

{
	"name": "haml",
	"displayName": "Better Haml",
	"description": "Better Haml syntax highlighting, auto close and suggestions",
	"version": "0.4.0",
	"publisher": "karunamurti",
	"engines": {
		"vscode": "^0.10.1"
	},
	"icon": "images/haml.png",
	"categories": [
		"Languages",
		"Snippets"
	],
	"license": "SEE LICENSE IN LICENSE.md",
	"contributes": {
		"languages": [
			{
				"id": "haml",
				"aliases": [
					"Ruby Haml",
          "Haml",
					"haml"
				],
				"extensions": [
					".haml"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "haml",
				"scopeName": "text.haml",
				"path": "./syntaxes/haml.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "haml",
				"path": "./snippets/snippets.json"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/karuna/ruby_haml_for_vscode.git"
	},
	"galleryBanner": {
		"color": "#888888",
		"theme": "dark"
	},
	"homepage": "https://github.com/karuna/ruby_haml_for_vscode",
	"bugs": {
		"url": "https://github.com/karuna/ruby_haml_for_vscode/issues",
		"email": "karuna.murti@gmail.com"
	},
	"__metadata": {
		"id": "31dab038-d2f7-47c2-894e-5c35684dbd0e",
		"publisherId": "14fb4112-a640-4645-8750-f1fab9c5e87a",
		"publisherDisplayName": "Karuna Murti"
	}
}
